{
	"version": 2,
	"env": {
		"DATABASE_URL": "postgresql://postgres:postgres@localhost:5432/sneakers?schema=public"
	},
	"builds": [
		{
			"src": "src/main.ts",
			"use": "@vercel/node"
		}
	],

	"routes": [
		{
			"src": "/(.*)",
			"dest": "src/main.ts",
			"methods": ["GET", "POST", "PUT", "PATCH", "DELETE"]
		}
	]
}
